@model Nop.Web.Areas.Admin.Models.LogisticsLeads.LogisticsLeadModel

@{
	//page title
    ViewBag.PageTitle = T("Admin.LogisticsLeads.Leads.MakeCopy").Text;
	//active menu item (system name)
    NopHtml.SetActiveMenuItemSystemName("Leads");
}

<form asp-controller="LogisticsLeads" asp-action="MakeCopy" method="post">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("Admin.LogisticsLeads.Leads.MakeCopy")
            <small>
                <i class="fas fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Admin.LogisticsLead.BackToList")</a>
            </small>
        </h1>
        <div class="float-right">
            <button type="submit" name="save" class="btn btn-primary">
                <i class="far fa-save"></i> 
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn btn-primary">
                <i class="far fa-save"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate", Model)
</form>