                    <a href="/Admin/Order/List?orderStatuses=10"><i class="far fa-eye"></i></a>
                    <p>4 Count</p>
                </div>
            </div>
        </div>
    </div>
    <script>
        var myChartCircle = new Chart("destinationService", {
            type: "doughnut",
            data: {
                datasets: [{
                    label: "Destination Service",
                    percent: 31,
                    backgroundColor: ["#FE7692"]
                }]
            },
            plugins: [{
                beforeInit: n => {
                    const t = n.data.datasets[0];
                    n.data.labels = [t.label];
                    t.data = [t.percent, 100 - t.percent]
                }
            }, {
                beforeDraw: n => {
                    var f = n.chart.width,
                        i = n.chart.height,
                        t = n.chart.ctx,
                        r;
                    t.restore();
                    r = (i / 75).toFixed(2);
                    t.font = r + "em sans-serif";
                    t.fillStyle = "#9b9b9b";
                    t.textBaseline = "middle";
                    var u = n.data.datasets[0].percent + "%",
                        e = Math.round((f - t.measureText(u).width) / 2),
                        o = i / 2;
                    t.fillText(u, e, o);
                    t.save()
                }
            }],
            options: {
                cutoutPercentage: 75,
                maintainAspectRatio: !1,
                legend: {
                    display: !1
                },
                tooltips: {
                    filter: n => n.index == 0
                }
