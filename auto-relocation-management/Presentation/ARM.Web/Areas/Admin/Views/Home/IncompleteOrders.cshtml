<div class="card card-primary card-outline" id="order-incomplete-report-card">
    <div class="card-header with-border">
        <h3 class="card-title">
            <i class="fas fa-hourglass-start"></i> Incomplete orders
        </h3>
        <div class="card-tools float-right">
            <button class="btn btn-tool" data-card-widget="collapse">
                <i class="fas fa-minus"></i>
            </button>
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $("#order-incomplete-report-card").on("click", 'button[data-card-widget="collapse"]', function () {
                var n = !$("#order-incomplete-report-card").hasClass("collapsed-card");
                saveUserPreferences("/Admin/Preferences/SavePreference", "Reports.HideOrderIncompleteReportCard", n)
            })
        })
    </script>
    <div class="card-body">
        <div class="incomplete-report-box totol-destination-service">
            <div class="graph-main">
                <div class="graph">
                    <div class="chartjs-size-monitor">
                        <div class="chartjs-size-monitor-expand">
                            <div class=""></div>
                        </div>
                        <div class="chartjs-size-monitor-shrink">
                            <div class=""></div>
                        </div>
                    </div>
                    <canvas id="destinationService" height="80" style="display: block; width: 80px; height: 80px;" width="80" class="chartjs-render-monitor"></canvas>
                </div>
                <div class="graph-info">
                    <div class="price">
                        $18,417.80 <span>Total destination services</span>
                    </div>
                </div>
            </div>
            <div class="graph-count-view">
                <a class="btn btn-xs btn-info btn-flat" href="/Admin/Order/List?orderStatuses=10,20,30&amp;paymentStatuses=10">View All</a>
                <div class="count-view">
                    <a href="/Admin/Order/List?orderStatuses=10,20,30&amp;paymentStatuses=10"><i class="far fa-eye"></i></a>
                    <p>5 Count</p>
                </div>
            </div>
        </div>
        <div class="incomplete-report-box totol-physical-service">
